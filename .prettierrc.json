{
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-astro"],
  "importOrder": ["^node:", "^@dassie/", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "semi": false,
  "experimentalTernaries": true,
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
